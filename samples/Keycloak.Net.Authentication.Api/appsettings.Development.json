{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "KeycloakUrl": "<FROM_USER_SECRET>",
  "RealmName": "Test",
  "JwtBearerOptions": {
    "Authority": "{KeycloakUrl}/realms/{RealmName}",
    "ClientId": "maui-client",
    "TokenValidationParameters": {
      "ValidAudience": "account",
      "ValidIssuer": "{KeycloakUrl}/realms/{RealmName}"
    }
  }
}
