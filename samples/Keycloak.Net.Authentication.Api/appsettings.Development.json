{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "KeycloakUrl": "<FROM_USER_SECRET>",
  "RealmName": "<FROM_USER_SECRET>",
  "JwtBearerOptions": {
    "Authority": "{KeycloakUrl}{RealmName}",
    "ClientId": "maui-client",
    "TokenValidationParameters": {
      "ValidAudience": "account"
    }
  },
  "JwtBearerOptionsTest": {
    "Authority": "{KeycloakUrl}{RealmName}",
    "ClientId": "maui-client",
    "TokenValidationParameters": {
      "ValidAudience": "account"
    }
  }
}
